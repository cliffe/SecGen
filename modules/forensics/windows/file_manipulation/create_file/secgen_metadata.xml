<?xml version="1.0"?>

<forensic xmlns="http://www.github/cliffe/SecGen/forensic"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.github/cliffe/SecGen/forensic">
  <name>Create file</name>
  <author>Jason Keighley</author>
  <module_license>Apache v2</module_license>
  <description>Create new file</description>

  <type>file_manipulation</type>
  <platform>windows</platform>

  <!--optional details-->
  <reference></reference>

  <read_fact>new_file_path</read_fact>
  <read_fact>new_file_contents</read_fact>

  <default_input into="new_file_path">
    <value>C:\Users\vagrant\Desktop\Hello.txt</value>
  </default_input>

  <default_input into="new_file_contents">
    <value>Test Test Test Test Test</value>
  </default_input>

</forensic>