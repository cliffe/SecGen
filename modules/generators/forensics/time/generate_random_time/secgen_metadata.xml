<?xml version="1.0"?>

<generator xmlns="http://www.github/cliffe/SecGen/generator"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.github/cliffe/SecGen/generator">
  <name>Generate random time</name>
  <author>Jason Keighley</author>
  <module_license>Apache v2</module_license>
  <description>Generates a random time between the given dates</description>

  <type>time_generator</type>
  <platform>windows</platform>

  <reference></reference>

  <!-- Number of Urls of a given type. -->
  <read_fact>date_start</read_fact>
  <read_fact>date_end</read_fact>

  <output_type>time</output_type>
</generator>